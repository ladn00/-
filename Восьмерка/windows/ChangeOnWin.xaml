<Window x:Class="Восьмерка.windows.ChangeOnWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Восьмерка.windows"
        mc:Ignorable="d"
        Title="ChangeOnWin" Height="180" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <Label Content="Введите мин. стоимость для агента: " Margin="10"/>
            <TextBox Margin="10" Height="30" Name="tb1"/>
            <Button Content="Подтвердить" Click="Accept_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
